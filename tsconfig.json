{
  "compilerOptions": {
   "target": "es2016",
   "experimentalDecorators": true,
   "emitDecoratorMetadata": true,
   "module": "commonjs",
   "moduleResolution": "node",
   "declaration": true,
   "resolveJsonModule": true,
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "strict": true,
   "noImplicitAny": true,
   "skipLibCheck": true
  }
}
